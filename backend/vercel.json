{
 "version": 2,
 "builds": [
  {
   "src": "src/server.js",
   "use": "@vercel/node",
   "config": {
        "includeFiles": [
          "node_modules/.prisma/client/**/*",
          "prisma/schema.prisma"
        ]
      }
  }
 ],
 "routes": [
  {
   "src": "/(.*)",
   "dest": "src/server.js"
  }
 ]
}